#include <iostream>
using namespace std;
int main(void)
{
	int x, y;
	char a, b;
	cin >> x >> a >> y >> b;
	cout << "x=" << x << "y=" << y << "a='" << a << "'" << "b='" << b << "'" << endl;
	return 0;
}


#include <iostream>
using namespace std;
int main()
{
	char a,b,c,d,e;
	cin.get(a);
	cin.get(b);
	cin.get(c);
	cin.get(d);
	cin.get(e);
	cout<<"a="<<a<<"b="<<b<<"c="<<c<<"d="<<d<<"e="<<e;
	return 0;
}


#include <iostream>
using namespace std;
int main()
{
	double a,b,c,d,s;
	cin>>a>>b>>c>>d>>s; 
	a=4.00;
	b=2.00;
	c=3.00;
	s=(a+b+c)/2;
	d= s* (s-a) * (s-b)*(s-c);
	cout<<"s="<<s;
	cout<<"d="<<d;
	return 0;
}
